import System

module TestingDemo:

	shared func Sqr(x):
		return x * x

	test Sqr:
		assert Sqr(9) == 81

	shared func Add(x, y):
		return x + y

	test Add:
		assert Add(2, 3) == 5

	shared func Equals(x, y):
		if x == y return true
		else return false

	test Equals:
		assert Equals(9, 9)
		assert !Equals(1, 2)

type Foo:

	func GetX():
		return 1234

	test Foo:
		f = Foo()
		assert f.GetX() == 1234
		f.Bar = "Dynamic member"
		assert f.Bar == "Dynamic member"